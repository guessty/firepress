{
  "name": "firestudio-default-project",
  "version": "1.0.0",
  "description": "firestudio default project",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "tslint --config tslint.js --project src/app && tslint --config tslint.js --project src/functions",
    "firebase:serve": "cd \"dist\" && firebase serve",
    "firebase:deploy": "cd \"dist/functions\" && npm install && cd \"../\" && firebase deploy",
    "next-fire:build": "next-fire build ./src/app -o ./dist",
    "next:dev": "node server.js ./src/app",
    "next:build": "next build ./src/app",
    "next:export": "next export ./src/app -o ./dist/public",
    "build": "npm run next:build && npm run next:export && npm run next-fire:build"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "7.0.0",
    "@zeit/next-typescript": "^1.1.1",
    "axios": "0.18.0",
    "core-js": "^2.5.7",
    "deep-object-diff": "^1.1.0",
    "express": "^4.16.4",
    "firebase-admin": "6.0.0",
    "firebase-functions": "2.0.5",
    "next": "^7.0.2",
    "next-fire": "^0.1.0",
    "next-spa": "^0.3.2",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-grid-flexbox": "0.7.0",
    "styled-components": "4.0.2",
    "styled-sanitize": "1.1.1",
    "typescript": "3.0.3",
    "unstated": "^2.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.0.0",
    "@types/next": "^7.0.3",
    "@types/react": "16.4.14",
    "@types/react-dom": "16.0.7",
    "@zeit/next-css": "^1.0.1",
    "autoprefixer": "^9.3.1",
    "babel-plugin-inline-import": "3.0.0",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-styled-components": "1.8.0",
    "sanitize.css": "^8.0.0",
    "tailwindcss": "^0.7.2",
    "tslint": "5.11.0",
    "tslint-react": "3.6.0"
  }
}
